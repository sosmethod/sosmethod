<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.sos.method" version="1.2.8">


    <name>SOS Method</name>
    <description>Meditation Reinvented</description>
    <author href="http://sosmethod.io" email="hello@thesosmethod.com">Act</author>
    <content src="index.html"/>
    <preference name="phonegap-version" value="cli-6.5.0"/>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="SplashScreenDelay" value="1000"/>
    <preference name="FadeSplashScreenDuration" value="500"/>
    <preference name="android-build-tool" value="gradle"/>
    <plugin name="phonegap-plugin-push" source="npm" spec="~1.8.4">
        <param name="SENDER_ID" value="402001388441"/>
    </plugin>
    <plugin name="cordova-plugin-console" source="npm" spec="~1.0.5"/>
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.3.1"/>
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="~4.0.1"/>
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.3.1"/>
    <icon src="assets/icon/icon.png"/>
    <platform name="android">
        <preference name="android-minSdkVersion" value="21"/>
        <icon src="assets/icon/android/drawable-ldpi-icon.png" density="ldpi"/>
        <icon src="assets/icon/android/drawable-mdpi-icon.png" density="mdpi"/>
        <icon src="assets/icon/android/drawable-hdpi-icon.png" density="hdpi"/>
        <icon src="assets/icon/android/drawable-xhdpi-icon.png" density="xhdpi"/>
        <icon src="assets/icon/android/drawable-xxhdpi-icon.png" density="xxhdpi"/>
        <icon src="assets/icon/android/drawable-xxxhdpi-icon.png" density="xxxhdpi"/>
        <splash src="assets/screen/android/drawable-land-ldpi-screen.png" density="land-ldpi"/>
        <splash src="assets/screen/android/drawable-land-mdpi-screen.png" density="land-mdpi"/>
        <splash src="assets/screen/android/drawable-land-hdpi-screen.png" density="land-hdpi"/>
        <splash src="assets/screen/android/drawable-land-xhdpi-screen.png" density="land-xhdpi"/>
        <splash src="assets/screen/android/drawable-land-xxhdpi-screen.png" density="land-xxhdpi"/>
        <splash src="assets/screen/android/drawable-land-xxxhdpi-screen.png" density="land-xxxhdpi"/>
        <splash src="assets/screen/android/drawable-port-ldpi-screen.png" density="port-ldpi"/>
        <splash src="assets/screen/android/drawable-port-mdpi-screen.png" density="port-mdpi"/>
        <splash src="assets/screen/android/drawable-port-hdpi-screen.png" density="port-hdpi"/>
        <splash src="assets/screen/android/drawable-port-xhdpi-screen.png" density="port-xhdpi"/>
        <splash src="assets/screen/android/drawable-port-xxhdpi-screen.png" density="port-xxhdpi"/>
        <splash src="assets/screen/android/drawable-port-xxxhdpi-screen.png" density="port-xxxhdpi"/>
    </platform>
    <platform name="ios">
        <icon src="assets/icon/ios/icon.png" platform="ios" width="57" height="57"/>
        <icon src="assets/icon/ios/icon@2x.png" platform="ios" width="114" height="114"/>
        <icon src="assets/icon/ios/icon-40.png" platform="ios" width="40" height="40"/>
        <icon src="assets/icon/ios/icon-40@2x.png" platform="ios" width="80" height="80"/>
        <icon src="assets/icon/ios/icon-50.png" platform="ios" width="50" height="50"/>
        <icon src="assets/icon/ios/icon-50@2x.png" platform="ios" width="100" height="100"/>
        <icon src="assets/icon/ios/icon-60.png" platform="ios" width="60" height="60"/>
        <icon src="assets/icon/ios/icon-60@2x.png" platform="ios" width="120" height="120"/>
        <icon src="assets/icon/ios/icon-60@3x.png" platform="ios" width="180" height="180"/>
        <icon src="assets/icon/ios/icon-72.png" platform="ios" width="72" height="72"/>
        <icon src="assets/icon/ios/icon-72@2x.png" platform="ios" width="144" height="144"/>
        <icon src="assets/icon/ios/icon-76.png" platform="ios" width="76" height="76"/>
        <icon src="assets/icon/ios/icon-76@2x.png" platform="ios" width="152" height="152"/>
        <icon src="assets/icon/ios/icon-small.png" platform="ios" width="29" height="29"/>
        <icon src="assets/icon/ios/icon-small@2x.png" platform="ios" width="58" height="58"/>
        <icon src="assets/icon/ios/icon-small@3x.png" platform="ios" width="87" height="87"/>
        <splash src="assets/screen/ios/Default-568h@2x~iphone.png" platform="ios" width="640" height="1136"/>
        <splash src="assets/screen/ios/Default-667h.png" platform="ios" width="750" height="1334"/>
        <splash src="assets/screen/ios/Default-736h.png" platform="ios" width="1242" height="2208"/>
        <splash src="assets/screen/ios/Default-Landscape-736h.png" platform="ios" width="2208" height="1242"/>
        <splash src="assets/screen/ios/Default-Landscape@2x~ipad.png" platform="ios" width="2048" height="1536"/>
        <splash src="assets/screen/ios/Default-Landscape~ipad.png" platform="ios" width="1024" height="768"/>
        <splash src="assets/screen/ios/Default-Portrait@2x~ipad.png" platform="ios" width="1536" height="2048"/>
        <splash src="assets/screen/ios/Default-Portrait~ipad.png" platform="ios" width="768" height="1024"/>
        <splash src="assets/screen/ios/Default@2x~iphone.png" platform="ios" width="640" height="960"/>
        <splash src="assets/screen/ios/Default~iphone.png" platform="ios" width="320" height="480"/>
    </platform>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
</widget>